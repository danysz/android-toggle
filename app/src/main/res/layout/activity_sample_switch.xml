<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?layoutBackground"
    android:orientation="vertical">

    <com.github.toggle.widget.LabeledSwitch
        android:id="@+id/switch8"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:textSize="12sp"
        app:colorThumbDisabled="#FFFFFF"
        app:colorThumbEnabled="#FFFFFF"
        app:colorTrackDisabled="#BDBDBD"
        app:colorTrackEnabled="@color/colorPrimary"
        app:noBackground="false"
        app:on="false" />
</androidx.appcompat.widget.LinearLayoutCompat>